pipeline {
	agent any
	
	parameters {
		string(name: 'docker_server' , defaultValue: '13.125.233.18' , description: 'Docker Server')
	}
	
	stages {
		stage('Build') {
			steps {
				echo '========================1. Maven Build ====================='
				
				sh 'mvn clean package'
				sh "docker build . -t myserver:${env.BUILD_ID} ./"
			}
		}
	}
}